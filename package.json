{
  "name": "jsnview",
  "version": "2.0.4",
  "description": "Ultra Fast and lightweight Json Viewer ~3KB with 0 Dependencies",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "scripts": {
    "copy-css": "cp build/index.css docs/jsnview.min.css",
    "copy-js": "cp build/index.js docs/jsnview.min.js",
    "watch": "rollup -c -w && yarn copy-js && yarn copy-css",
    "build": "rollup -c && yarn copy-js && yarn copy-css"
  },
  "devDependencies": {
    "rollup": "^2.70.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haikelfazzani/json-view"
  },
  "bugs": {
    "url": "https://github.com/haikelfazzani/json-view/issues"
  },
  "homepage": "https://github.com/haikelfazzani/json-view#readme",
  "keywords": [
    "json",
    "view",
    "json viewer",
    "format",
    "fast",
    "lightweight",
    "umd",
    "esm"
  ],
  "author": "Haikel Fazzani",
  "license": "MIT"
}